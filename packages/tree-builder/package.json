  },
  "engines": {
    "node": ">=14.x",
    "npm": ">=6.14.x"
  },
  "devEngines": {
    "node": ">=14.x",
    "npm": ">=6.14.x"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/jest": "27.0.2",
    "@types/node": "^17.0.8",
    "debug": "^4.3.4"
  }
